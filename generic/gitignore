/config/database.yml
/config/application.yml

.sass-cache/

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore Spring files
/spring/*.pid

# Ignore Mac OS X sys files
*.DS_Store
*/.DS_Store

